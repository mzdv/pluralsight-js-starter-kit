{
  "name": "pluralsight-js-starter-kit",
  "version": "1.0.0",
  "description": "Pluralsight course JS starter kit",
  "main": "src/index.js",
  "scripts": {
    "prestart": "node buildHarness/prestart.js",
    "run": "node dist/index.js",
    "start": "npm-run-all security-check compile lint test run",
    "lint": "esw src buildHarness",
    "lint:watch": "npm run lint -- --watch",
    "security-check": "nsp check",
    "expose": "lt --port 3000",
    "compile:typescript": "tsc",
    "compile": "npm-run-all compile:typescript compile:cleanup",
    "compile:cleanup": "rimraf dist/test",
    "test": "nyc --require ts-node/register tape test/**/*.ts | tap-spec",
    "coverage": "nyc report",
    "coverage:html": "nyc report --reporter=lcov",
    "clean": "rimraf dist coverage .nyc_output"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mzdv/pluralsight-js-starter-kit.git"
  },
  "keywords": [
    "starter",
    "kit"
  ],
  "author": "Milos Zivadinovic",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mzdv/pluralsight-js-starter-kit/issues"
  },
  "homepage": "https://github.com/mzdv/pluralsight-js-starter-kit#readme",
  "devDependencies": {
    "@types/tape": "^4.2.30",
    "chalk": "^2.1.0",
    "eslint": "^4.8.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.2.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-watch": "^3.1.2",
    "localtunnel": "^1.8.3",
    "npm-run-all": "^4.1.1",
    "nsp": "^2.8.1",
    "nyc": "^11.2.1",
    "rimraf": "^2.6.2",
    "tap-nyan": "^1.1.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.8.0",
    "tape-watch": "^2.3.0",
    "ts-node": "^3.3.0",
    "tslint": "^5.7.0",
    "typescript": "^2.5.3",
    "typescript-eslint-parser": "^8.0.0"
  },
  "dependencies": {},
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ]
  }
}
